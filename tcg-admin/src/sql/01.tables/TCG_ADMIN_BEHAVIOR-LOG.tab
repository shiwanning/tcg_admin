CREATE TABLE BEHAVIOR_LOG(
  SYSID NUMBER,
  IP_ADDRESS VARCHAR2(15),
  OPERATOR_NAME VARCHAR2(30),
  URL VARCHAR2(256) NOT NULL,
  RESOURCE_TYPE NUMBER(3,0),
  PARAMS VARCHAR2(3000),
  BROWSER VARCHAR2(256),
  RESOURCE_ID NUMBER(6,0),
  START_PROC_DATE TIMESTAMP,
  END_PROC_DATE TIMESTAMP,
  CONSTRAINT BEHAVIOR_LOG_PK PRIMARY KEY(SYSID)
);
