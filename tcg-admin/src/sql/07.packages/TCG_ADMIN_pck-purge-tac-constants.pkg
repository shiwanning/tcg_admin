create or replace PACKAGE PCK_PURGE_TAC_CONSTANT IS
    /*batch commit 筆數*/
    PURGE_BATCH_PART_COUNT CONSTANT NUMBER:=100000;
    /*預設回傳筆數*/
    PURGE_DEFAULT_COUNT CONSTANT NUMBER:=0;
    /*成功 Remark*/
    PURGE_PURGE_SUCCESS CONSTANT VARCHAR2(10):='SUCCESS';
    /*失敗 Remark*/
    PURGE_PURGE_FAILED CONSTANT VARCHAR2(10):='FAILED';
END PCK_PURGE_TAC_CONSTANT;